<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.CustomerUpdateController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Rectangle fill="#252525" height="449.0" stroke="WHITE" strokeType="INSIDE" width="400.0" />
            <Text fill="WHITE" layoutX="61.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Update Customer">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Line endX="300.0" layoutX="50.0" layoutY="67.0" />
            <TextField fx:id="custID" disable="true" layoutX="167.0" layoutY="88.0" prefHeight="25.0" prefWidth="37.5" />
            <TextField fx:id="custName" layoutX="167.0" layoutY="133.0" />
            <TextField fx:id="custAddress" layoutX="167.0" layoutY="178.0" />
            <ComboBox fx:id="custCountryCombo" layoutX="167.0" layoutY="223.0" onAction="#pullFLD" prefWidth="150.0" />
            <ComboBox fx:id="custFLDCombo" layoutX="167.0" layoutY="268.0" prefWidth="150.0" />
            <TextField fx:id="custPostal" layoutX="169.0" layoutY="313.0" />
            <TextField fx:id="custPhone" layoutX="167.0" layoutY="358.0" />
            <Text fill="#3392e6" layoutX="75.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer ID:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fill="#3392e6" layoutX="115.0" layoutY="151.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fill="#3392e6" layoutX="102.0" layoutY="196.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Address:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fill="#3392e6" layoutX="103.0" layoutY="241.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Country:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fill="#3392e6" layoutX="62.0" layoutY="286.0" strokeType="OUTSIDE" strokeWidth="0.0" text="State/Province:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fill="#3392e6" layoutX="79.0" layoutY="331.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Postal Code:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Text fill="#3392e6" layoutX="59.0" layoutY="376.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone Number:">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Button fx:id="submitButton" layoutX="203.0" layoutY="403.0" minWidth="75.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
            <Button layoutX="15.0" layoutY="23.0" minHeight="34.0" minWidth="34.0" mnemonicParsing="false" onAction="#goBack" prefHeight="34.0" prefWidth="34.0" style="-fx-background-color: #252525;">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0">
                     <image>
                        <Image url="@Images/BackArrow.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
