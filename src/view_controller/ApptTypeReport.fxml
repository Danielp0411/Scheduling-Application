                  <Font size="14.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="augRadio" layoutX="113.0" layoutY="118.0" mnemonicParsing="false" onAction="#pullTypeTable" text="August" textFill="#3392e6" toggleGroup="$months">
