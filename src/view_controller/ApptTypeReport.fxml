<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.ApptTypeReportController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Rectangle fill="#252525" height="276.0" stroke="WHITE" strokeType="INSIDE" width="455.0" />
            <TableView fx:id="typeTable" layoutX="234.0" layoutY="88.0" prefHeight="168.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="typeCol" prefWidth="150.0" text="Type" />
                <TableColumn fx:id="countCol" prefWidth="50.0" text="#" />
              </columns>
            </TableView>
            <Text fill="WHITE" layoutX="86.0" layoutY="53.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Appt Type Report">
